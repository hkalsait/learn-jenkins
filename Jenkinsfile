pipeline {
  agent any
  
    stages {

      stage ("INITIAL") {
        steps {
          echo "Initialize steps..."
        }
      }
      
      stage ("BUILD") {
        steps {
          echo "Building the app..."
        }
      }
      
      stage ("TEST") {
        steps {
          echo "Testing the app..."
        }
      }

      stage ("DEPLOY") {
        steps {
          echo "deploying the app..."
        }
      }

      stage ("CLEANUP") {
        steps {
          echo "Cleaning up the app..."
        }
      }
    }
}  
